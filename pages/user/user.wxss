page{
    background-color: rgb(240, 236, 236);
}
.container{
    padding: 10rpx;
}
.user_info{
    background-color: #fff;
    display: flex;
    width: 100%;
    margin-bottom: 20rpx;
    justify-content: space-between;
}
.avator_warp{
    display: flex;
    flex: 3;
    justify-content: center;
    align-items: center;
}
.avator_warp image{
   width: 100%;
   max-height: 200rpx;
}
.user_detail{
    display: flex;
    flex:5;
    flex-direction: column;
    justify-content: space-around;
}
.more{
    display: flex;
    flex:1;
    justify-content: center;
    align-items: center;
}
.more image{
    width:30%;
    max-height: 50rpx;
}
.mine_warp{
    background-color: #fff;
    border-bottom:  0.1rpx solid rgb(189, 189, 189);
    display: flex;
    width: 100%;
    height: 100rpx;
    justify-content: center;
    align-items: center;
}
.to_mine{
    margin-left: 20rpx;
    flex:8;
}
.credit_warp{
    background-color: #fff;
    margin-bottom: 20rpx;
    display: flex;
    width: 100%;
    height: 100rpx;
    justify-content: center;
    align-items: center;
}
.to_credit{
    margin-left: 20rpx;
    flex:8;
}